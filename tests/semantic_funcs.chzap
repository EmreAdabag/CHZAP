/* CHZAP functions */

int a;

float add_f(float a, float b) {
    return a + b;
}

float sum_thing(int a, int b, function(float, float) -> float f) {
    float c;
    float d;
    c = 1.0;
    d = 2.0;
    return f(c, d) + a + b;
}

function(void) -> int func_decl;


int main() {
    int a;
    a = 2;

    sum_thing(a, 3, function(float x, float y) -> float { return x + y; });
}

