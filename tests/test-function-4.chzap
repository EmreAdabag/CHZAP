int sum(int a, int b, function(int, int) -> int f) {
    return f(a, b);
}

function(void) -> int func_decl;

int main() {
    int a = 3;
    int b = 5;
    int s = sum(a, b, function(int x, int y) -> int { return x + y; });
}
